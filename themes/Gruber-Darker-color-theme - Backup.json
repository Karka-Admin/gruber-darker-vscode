			"settings": {
				"foreground": "#cc8c3c"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"storage.modifier.pointer",
				"keyword.operator",
				"entity.name.function.preprocessor",
				"keyword.operator.bitwise.shift",
